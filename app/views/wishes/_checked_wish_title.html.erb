<div class="flex justify-between items-center basis-11/12" id= <%= "wish_#{wish.id}" %> >
  <div class="form-control">
    <%= link_to uncheck_path(wish), data: { turbo_stream: true, turbo_method: :post } do %>
      <label class="label cursor-pointer justify-normal">
        <input type="checkbox" checked="checked" class="checkbox checkbox-accent mr-3" />
        <span class="label-text text-xl text-zinc-400 line-through">
          <%= wish.title %>
        </span>
      </label>
    <% end %>
  </div>
  <!-- 作成日 -->
  <div class="text-zinc-400 line-through ml-3"><%= l wish.created_at %></div>
</div>